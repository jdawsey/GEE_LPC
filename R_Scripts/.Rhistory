p <- ggplot(mpg, aes(displ, hwy)) + geom_point()
p + facet_grid(. ~ cyl)
p + facet_grid(drv ~ .)
p + facet_grid(drv ~ cyl)
p + facet_wrap(~ class)
p + facet_grid(. ~ cyl)
p + facet_grid(drv ~ .)
p + facet_grid(drv ~ cyl)
p + facet_wrap(~ class) #
#jitter
p + geom_jitter()
ggplot(mpg, aes(class, hwy)) + geom_boxplot()
p <- ggplot(mpg, aes(reorder(class, hwy), hwy))
p
p + geom_boxplot()
?reorder
p <- ggplot(mpg, aes(reorder(class, hwy, median), hwy)) # reorders
p + geom_boxplot()
?reorder
p + geom_violin() + geom_boxplot()
